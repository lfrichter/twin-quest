{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build && vite build --ssr",
        "dev": "vite",
        "test": "vitest",
        "typecheck": "vue-tsc --noEmit",
        "watch:types": "vue-tsc --noEmit --watch",
        "watch:build": "vite build --watch",
        "watch:build:ssr": "vite build --ssr --watch",
        "watch:build:all": "concurrently \"npm:watch:build\" \"npm:watch:build:ssr\""
    },
    "devDependencies": {
        "@inertiajs/vue3": "^2.0",
        "@tailwindcss/forms": "^0.5.7",
        "@tailwindcss/typography": "^0.5.10",
        "@tailwindcss/vite": "^4.0.0",
        "@types/node": "^22.15.17",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/server-renderer": "^3.4.0",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.16",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "jsdom": "^26.1.0",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.8.3",
        "vite": "^6.2.4",
        "vitest": "^3.1.3",
        "vue": "^3.3.13",
        "vue-tsc": "^2.2.10"
    },
    "dependencies": {
        "pinia": "^3.0.2",
        "vee-validate": "4.15.0",
        "yup": "^1.6.1",
        "ziggy-js": "^2.5.2"
    }
